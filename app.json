{
  "expo": {
    "name": "Hopinn",
    "slug": "hopin-rider",
    "version": "0.0.21",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.hopinnRider",
      "config": {
        "googleMapsApiKey": "AIzaSyDDGCRsdawsguNpqCvTI-zlgCBDz2H8zVY"
      },
      "infoPlist": {
        "NSLocationUsageDescription": "Hopinn collects location data to make sure a faultless and express ride."
      }
    },
    "android": {
      "package": "com.company.hopinnRider",
      "config": {
        "googleMaps": {
          "apiKey": "<android maps key>"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": [
      "android",
      "ios"
    ],
    "extra": {
      "eas": {
        "projectId": "a3dbc6e8-37e9-4ef1-a7fe-294767d8b2ba"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME) collects location data to make sure a faultless and express ride."
        }
      ]
    ]
  }
}